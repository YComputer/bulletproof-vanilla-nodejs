{
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src",
            "config": "./src/config",
            "db": "./src/db",
            "declarations": "./src/declarations",
            "utility": "./src/utility",
            "helpers": "./src/helpers"
          }
        }
      ]
    ],
    "presets": [
      "@babel/preset-env"
    ]
  }